@model IEnumerable<ApplicationCore.Models.MovieCardModel>

<div class="rounded">
    <div class="container-fluid">
        <div class="row">
            @foreach (var movie in Model)
            {
                <div class="col-xl-2 col-lg-3 col-md-4 col-6 mb-4">
                    <partial name="_MovieCard" model="movie" />
                </div>
            }
        </div>
    </div>
</div>
